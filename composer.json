{
  "name": "rocont/laravel-notification-channel-compass",
  "description": "Laravel Notification Channel for Compass Userbot",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mikhail Margasov",
      "email": "m.margasov@rocont.ru",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/Rocont/laravel-notification-channel-compass",
  "require": {
    "php": "^8.1",
    "illuminate/notifications": "^10|^11|^12",
    "illuminate/support": "^10|^11|^12",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5|^11.0",
    "mockery/mockery": "^1.6",
    "orchestra/testbench": "^8.0|^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Rocont\\CompassChannel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rocont\\CompassChannel\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rocont\\CompassChannel\\CompassServiceProvider"
      ]
    }
  }
}
